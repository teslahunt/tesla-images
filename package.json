{
  "name": "tesla-images",
  "description": "Generate Tesla Model S/3/X/Y 3D photo visualizations",
  "homepage": "https://github.com/teslahunt/tesla-images",
  "version": "1.6.1",
  "main": "src/index.js",
  "author": {
    "email": "hello@teslahunt.io",
    "name": "Tesla Hunt",
    "url": "https://teslahunt.io"
  },
  "contributors": [
    {
      "name": "Kiko Beats",
      "email": "josefrancisco.verdu@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teslahunt/tesla-images.git"
  },
  "bugs": {
    "url": "https://github.com/teslahunt/tesla-images/issues"
  },
  "keywords": [
    "3d",
    "gallery",
    "generation",
    "images",
    "model-3",
    "model-s",
    "model-x",
    "model-y",
    "model3",
    "models",
    "modelx",
    "modely",
    "photo",
    "tesla",
    "tesla-api",
    "teslaapi",
    "teslamotors",
    "visualization",
    "viz"
  ],
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@ksmithut/prettier-standard": "latest",
    "ava": "5",
    "browserless": "latest",
    "c8": "latest",
    "ci-publish": "latest",
    "finepack": "latest",
    "git-authors-cli": "latest",
    "github-generate-release": "latest",
    "nano-staged": "latest",
    "p-map": "4",
    "puppeteer": "latest",
    "reachable-url": "latest",
    "signal-exit": "latest",
    "simple-git-hooks": "latest",
    "standard": "latest",
    "standard-markdown": "latest",
    "standard-version": "latest",
    "superlock": "latest"
  },
  "engines": {
    "node": ">= 18"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "clean": "rm -rf node_modules",
    "contributors": "(npx git-authors-cli && npx finepack && git add package.json && git commit -m 'build: contributors' --no-verify) || true",
    "lint": "standard-markdown README.md && standard",
    "postrelease": "npm run release:tags && npm run release:github && (ci-publish || npm publish --access=public)",
    "prerelease": "npm run update:check && npm run contributors",
    "pretest": "npm run lint",
    "release": "standard-version -a",
    "release:github": "github-generate-release",
    "release:tags": "git push --follow-tags origin HEAD:master",
    "test": "c8 ava"
  },
  "license": "MIT",
  "ava": {
    "timeout": "2m",
    "files": [
      "test/**/*.js",
      "!test/util.js"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        0
      ]
    }
  },
  "nano-staged": {
    "*.js": [
      "prettier-standard",
      "standard --fix"
    ],
    "*.md": [
      "standard-markdown"
    ],
    "package.json": [
      "finepack"
    ]
  },
  "simple-git-hooks": {
    "commit-msg": "npx commitlint --edit",
    "pre-commit": "npx nano-staged"
  }
}
